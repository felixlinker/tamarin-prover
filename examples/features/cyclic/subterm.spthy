theory Subterms
begin

functions: h/1

rule Start:
  [ Fr(~id), Fr(~x) ] --[ L(~id, ~x) ]-> [ Loop(~id, ~x) ]

rule Loop:
  [ Loop(~id, x) ] --[ L(~id, h(x)) ]-> [ Loop(~id, h(x)) ]

lemma ST:
  "All id x1 x2 #t1 #t2.
        L(id, x1) @ #t1 & L(id, x2) @ #t2 & not (#t1 = #t2)
    ==> (x1 << x2) | (x2 << x1)"

end
